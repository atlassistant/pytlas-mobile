{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": "off",
        "import/prefer-default-export": "off"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        }
    }
}